#
a --> (b && c);
(b || d) --> e;
d || a;
|- e;
#
(a && b) --> (c && d);
b --> ~d;
|- ~a || ~b
#
(a || b) --> (c && d);
(c || e) --> (d --> ~a);
|- ~a;
#
w --> (x --> y);
w --> x:
~z || (y || w);
|- ~z || y;
#
(b --> a) --> c;
(a --> b) --> c;
|- c;
#
~e;
|- ~(((a --> b) &&  (b --> c)) && ((a --> c) --> e));
#
(~t || u) --> (w && x);
(w || y) --> (x --> t);
|- t;
#
(a && b) || c;
~c || b;
|- a --> b;
#
(a && b) --> (c || d);
(a --> c) --> (b --> d);
b;
|- d;
#
a --> (~b --> c);
(a --> b) --> c;
|- c;
#
a <--> (b <--> c);
|- (a <--> b) <--> c;
#
(a && b) <--> ~c;
a --> b;
|- c <--> ~a;
#
quit;
