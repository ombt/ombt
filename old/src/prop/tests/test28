#
# test cases from chapter 4 in the book Deduction: Introductory 
# Symbolic Logic, by D. Bonevac.
#
#############################################################################
#
p || q;
~p;
|- q;
#
p || q;
p --> r;
q --> s;
|- r || s;
#
(p && q) || (~p && ~q);
|- p <--> q;
#
~p || ~q;
|- q --> ~p;
#
~p || ~r;
|- ~(p && r);
#
p || q;
~p || ~q;
|- p <--> ~q;
#
(p || q) || r;
|- p || (q || r);
#
p && (q || r);
|- (p && q) || (p && r);
#
(r && ~p) || (q && r);
|- (p --> q) && r;
#
p || q;
r || s;
~(p || s);
|- (q && r) || p1;
#
p;
~s || ~p;
p --> r;
|- ~s && r;
#
p || q;
p --> r;
~r;
|- q;
#
~p || q;
~q || r;
~r;
|- ~p;
#
(p && ~q) --> ~r;
r;
|- ~p || (p && q);
#
~s || (s && p);
(s --> p) --> r;
|- r;
#
p --> ~q;
~p || r;
q;
(q && r) --> p;
|- ~r;
#
~(p && ~q) || ~p;
|- p --> q;
#
p && s;
p --> (~s || r);
|- r;
#
~p || q;
~q;
~p --> r;
|- r;
#
p --> q;
r --> p;
|- ~r || q;
#
r --> p;
~r --> q;
q --> s;
|- p || s;
#
p --> ~q;
r;
r --> (q || ~s);
|- s --> ~p;
#
p --> (~r || s);
p --> ~s;
|- p --> ~r;
#
p --> ~q;
~p --> ~r;
r || ~s;
|- ~q || ~s;
#
p && ~s;
r --> s;
p --> (q || r);
|- q;
#
p && q;
r && ~s;
q --> (p --> p1);
p1 --> (r --> (s || q1));
|- q1;
#
p && q;
p --> (s || r);
~(r && q);
|- s;
#
p || (q || s);
s1 && ~s2;
~(~s1 || s2) --> ~p;
(s --> r) && ~r;
|- q;
#
p && (~q && ~p1);
p --> (s --> r);
s --> (r <--> (p1 || q));
|- ~s;
#
s --> p;
(s && p) --> q;
r --> s1;
r || s;
|- q || s1;
#
p || (r || q);
(r --> s1) && ( q --> s2);
(s1 || s2) --> (p || q);
~p;
|- q;
#
p --> (s && r);
(r || ~s) --> (q && q1);
q1 <--> q2;
|- p --> q2;
#
(p && q) || (q && r);
|- ~q --> s;
#
(p || q) && r;
q --> s;
|- ~p --> (r --> s);
#
~p || (q && r);
(r || ~q) --> (s && q1);
(q1 && q2) || ~(q1 || q2);
|- ~p || q2;
#
~(~p && q) && (p <--> ~q);
|- p <--> (q --> r);
#
quit;
