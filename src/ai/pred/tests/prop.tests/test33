#
A --> (B && C);
(B || D) --> E;
D || A;
|- E;
#
(A && B) --> (C && D);
B --> ~D;
|- ~A || ~B
#
(A || B) --> (C && D);
(C || E) --> (D --> ~A);
|- ~A;
#
W --> (X --> Y);
W --> X:
~Z || (Y || W);
|- ~Z || Y;
#
(B --> A) --> C;
(A --> B) --> C;
|- C;
#
~E;
|- ~(((A --> B) &&  (B --> C)) && ((A --> C) --> E));
#
(~T || U) --> (W && X);
(W || Y) --> (X --> T);
|- T;
#
(A && B) || C;
~C || B;
|- A --> B;
#
(A && B) --> (C || D);
(A --> C) --> (B --> D);
B;
|- D;
#
A --> (~B --> C);
(A --> B) --> C;
|- C;
#
A <--> (B <--> C);
|- (A <--> B) <--> C;
#
(A && B) <--> ~C;
A --> B;
|- C <--> ~A;
#
quit;
