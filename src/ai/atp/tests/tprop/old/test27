#
# test cases from chapter 4 in the book Deduction: Introductory 
# Symbolic Logic, by D. Bonevac.
#
#############################################################################
#
p --> q;
q --> r;
|- p --> r;
#
p --> q;
q --> r;
|- ~r --> ~p;
#
p --> q;
~p --> r;
|- ~q --> r;
#
p --> q;
~p --> r;
|- ~r --> q;
#
p --> r;
q --> ~r;
|- p --> ~q;
#
p <--> (q && p);
|- p --> q;
#
p --> q;
~(p --> r);
|- ~(q --> r);
#
p --> q;
p --> r;
|- p --> (q && r);
#
p <--> q;
p <--> r;
|- q <--> r;
#
p --> q;
~q;
|- ~p
#
p <--> q;
~p;
|- ~q;
#
(p --> q) --> p;
|- p;
#
p --> q;
|- p <--> (p && q);
#
p && ~q;
r --> (r && q);
~r --> s;
|- s;
#
s --> (r && p);
q --> (~r && ~p1);
|- (q && s) --> p2;
#
p --> (q && (~r --> ~p));
q <--> ~r;
|- ~p;
#
(p --> q) --> (r --> s);
~(p && q) --> s;
|- ~s --> ~r;
#
(s && ~r) --> ~p;
(q --> ~s) <--> ~p;
|- p <--> (r && (s && q));
#
p <--> q;
|- ~p <--> ~q;
#
(p <--> q) <--> (p <--> r);
|- (q <--> r);
#
quit;

