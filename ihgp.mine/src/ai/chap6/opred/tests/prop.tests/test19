#
# PROBLEMS FROM DEDUCTION: INTRODUCTORY SYMBOLIC LOGIC BY
# DANIEL BONEVAC, CHAPTER 4.
#
~(P --> ~Q);
R --> (~P || ~Q);
(R || S) <--> S1;
|- S1 <--> S;
#
~(~P && Q);
(P || Q) --> ~(Q1 && S);
Q <--> S;
|- Q1 --> ~(Q || S);
#
P --> (Q || R);
(~Q && Q1) || (S --> P);
~(~R --> ~P);
|- ~S || Q;
#
P --> (Q --> R);
(~Q <--> S) --> ~P;
P || Q1;
|- (~R && S) --> Q1;
#
(P <--> Q) <--> R;
~(P <--> ~R);
(Q || ~S) --> S1;
|- S1;
#
~S --> ~S2;
(S && S1) --> (P <--> Q);
~(~P || Q);
|- S1 --> ~S2;
#
(Q <--> ~P) --> ~R;
(~Q && S) || (P && Q1);
(S || Q1) --> R;
|- P --> Q;
#
(P && ~R) <--> (S || ~Q);
S1 && ((~S && ~R) --> P);
((S1 --> Q) || (S1 --> R)) && (((P && Q) && S) --> R);
|- Q && R;
#
quit;
