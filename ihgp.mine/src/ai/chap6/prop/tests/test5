#
# solved problems in chapter 3, propositional calculus, in schaum's.
#
~p --> (q --> r), ~p, q;
|- r;
#
~p --> ~~q, ~~~p;
|- q;
#
p --> (q && r), p;
|- p && q;
#
p && q;
|- q && p;
#
(p && q) --> ( r && s), ~~p, q;
|- s;
#
p;
|- P && p;
#
p;
|- (p || q) && (p || r);
#
p;
~~(p --> q);
|- (r && s) || q;
#
p;
|- p || p;
#
(p || q) && (p || r), p --> s, q --> s, p --> t, r --> t;
|- s && t;
#
p || p, p --> (q && r);
|- r;
#
w <--> (s1 || s2), s2;
|- w;
#
p --> q;
(p --> q) --> (q --> p);
|- (p <--> q);
#
p <--> q;
|- q <--> p;
#
# supplementary problems V in schaums, page 70.
#
p <--> q, q <--> r;
|- p <--> r;
#
p <--> q;
|- ~p <--> ~q;
#
~p || q;
|- ~(p && ~q);
#
p --> q, p --> ~q;
|- ~p;
#
(p --> q) && (p --> r);
|- p --> (q && r);
#
p --> q;
|- (p && r) --> (q && r);
#
p --> q;
|- (p || r) --> (q || r);
#
~p --> p;
|- p;
#
~p;
|- p --> q;
#
p && q;
|- p --> q;
#
# supplementary problems VI in schaums, page 71.
#
|- p --> p;
#
|- p --> (q --> (p && q));
#
|- ~(p <--> ~p);
#
|- (p --> q) --> (~q --> ~p);
#
|- (p && q) || (~p || ~q);
#
|- q --> (p || ~p);
#
|- (p && ~p) --> q;
#
|- p || (p --> q);
#
|- ~p || (q --> p);
#
|- (p --> q) || (q --> p);
#
# supplementary problems VII in schaums, page 71.
#
|- (p && q) <--> ~(~p || ~q);
#
|- (p || q) <--> ~(~p && ~q);
#
|- (p && q) <--> ~(p --> ~q);
#
|- (p || q) <--> ~p --> q;
#
|- p <--> ((p && q) || (p && ~q));
#
|- ~(p --> q) <--> (p && ~q);
#
|- (p <--> q) <--> ((p && q) || (~p && ~q));
#
|- ~(p <--> q) <--> ((~p && q) || (p && ~q));
#
|- (p && ~p) <--> (q && ~q);
#
|- (p || ~p) <--> (q || ~q);
#
quit;
