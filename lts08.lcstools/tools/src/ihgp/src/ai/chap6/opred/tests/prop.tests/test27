#
# TEST CASES FROM CHAPTER 4 IN THE BOOK DEDUCTION: INTRODUCTORY 
# SYMBOLIC LOGIC, BY D. BONEVAC.
#
#############################################################################
#
P --> Q;
Q --> R;
|- P --> R;
#
P --> Q;
Q --> R;
|- ~R --> ~P;
#
P --> Q;
~P --> R;
|- ~Q --> R;
#
P --> Q;
~P --> R;
|- ~R --> Q;
#
P --> R;
Q --> ~R;
|- P --> ~Q;
#
P <--> (Q && P);
|- P --> Q;
#
P --> Q;
~(P --> R);
|- ~(Q --> R);
#
P --> Q;
P --> R;
|- P --> (Q && R);
#
P <--> Q;
P <--> R;
|- Q <--> R;
#
P --> Q;
~Q;
|- ~P
#
P <--> Q;
~P;
|- ~Q;
#
(P --> Q) --> P;
|- P;
#
P --> Q;
|- P <--> (P && Q);
#
P && ~Q;
R --> (R && Q);
~R --> S;
|- S;
#
S --> (R && P);
Q --> (~R && ~P1);
|- (Q && S) --> P2;
#
P --> (Q && (~R --> ~P));
Q <--> ~R;
|- ~P;
#
(P --> Q) --> (R --> S);
~(P && Q) --> S;
|- ~S --> ~R;
#
(S && ~R) --> ~P;
(Q --> ~S) <--> ~P;
|- P <--> (R && (S && Q));
#
P <--> Q;
|- ~P <--> ~Q;
#
(P <--> Q) <--> (P <--> R);
|- (Q <--> R);
#
quit;

