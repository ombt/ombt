RTRV-EQPT;
RTRV-INFO-EQPT;
RTRV-EQPT-ALL;
RTRV-PM-CC;
RTRV-ALM-ALL;
INIT-SYS:TELICA::::10;
RTRV-CHASSIS-EQPT;
RTRV-CALL-CAPTVERBOSE;
RTRV-TRAFFICSTATS;
ED-SYS-SECU:::::SSHENABLED=n;
ED-SYS-SECU:::::SSHENABLED=y;
RTRV-PRFL-SIP;
RTRV-SLHR-SCP;

===================EXTRACT CPU FILES================================

EXEC-LS:::::-1;
EXEC-CD:::::/home1/;
EXEC-LS:::::-1;
EXEC-TAR-EXTRACT::COM:::6.2.1.2.86_cpu.tar.gz;
EXEC-TAR-EXTRACTSTBY::COM:::/home1/6.2.1.2.86_cpu.tar.gz,/home1/;


===================EXTRACT IOM FILES================================
EXEC-CD:::::/;
EXEC-LS:::::-1;
EXEC-TAR-EXTRACT::COM:::6.2.1.2.30_ds1_2.tar.gz;
EXEC-TAR-EXTRACT::COM:::6.2.1.2.7_ena.tar.gz;
EXEC-TAR-EXTRACT::COM:::6.2.1.2.24_octds3_2.tar.gz;
EXEC-TAR-EXTRACT::COM:::6.2.1.2.25_trids3.tar.gz;
EXEC-TAR-EXTRACT::COM:::6.2.1.2.14_vs2.tar.gz;

EXEC-TAR-EXTRACTSTBY::COM:::/6.2.1.2.30_ds1_2.tar.gz,/;
EXEC-TAR-EXTRACTSTBY::COM:::/6.2.1.2.7_ena.tar.gz,/;
EXEC-TAR-EXTRACTSTBY::COM:::/6.2.1.2.24_octds3_2.tar.gz,/;
EXEC-TAR-EXTRACTSTBY::COM:::/6.2.1.2.25_trids3.tar.gz,/;
EXEC-TAR-EXTRACTSTBY::COM:::/6.2.1.2.14_vs2.tar.gz,/;

===================BACKUP ======================================

EXEC-BACKUP-PLEXUS::COM:::135.1.104.143,root,plexus9000,/FTPRoot;
EXEC-BACKUP-PLEXUS::COM:::135.1.104.144,root,plexus9000,/FTPRoot;

===========UPGRADE SP-A/B (SP-A active)==========================

ED-EQPT::SP-B:::SWVERSION=6.2.1.2.86
RMV-EQPT::SP-B;
wait 5 minutes
EXEC-CD:::::/home1/;
EXEC-LS:::::-1;
EXEC-RUN-UPGRADE:ihlcs3:SP-B
RST-EQPT::SP-B;

ED-EQPT::SP-A:::SWVERSION=6.2.1.2.86
EXEC-LS:::::-l,/Telica/swCPU/CurrRel;
SW-TOPROTN-EQPT::SP-A;
RMV-EQPT::SP-A;
wait 5 miniutes
EXEC-CD:::::/home1/;
EXEC-LS:::::-1;
EXEC-RUN-UPGRADE:ihlcs3:SP-A
RST-EQPT::SP-A;

