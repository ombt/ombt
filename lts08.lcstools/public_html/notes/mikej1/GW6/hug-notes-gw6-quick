RTRV-STATUS-MG;
RTRV-EQPT;
RTRV-INFO-EQPT;
RTRV-EQPT-ALL;
RTRV-PM-DB;
RTRV-PM-DBTABLE;
RTRV-ALM-ALL;
INIT-SYS:TELICA::::10;
RTRV-CHASSIS-EQPT;
RTRV-CALL-CAPTVERBOSE;
RTRV-TRAFFICSTATS;
ED-SYS-SECU:::::SSHENABLED=n;
ED-SYS-SECU:::::SSHENABLED=y;

===================BACKUP ======================================

EXEC-BACKUP-PLEXUS::COM:::135.1.104.195,root,plexus9000,/FTPRoot;
EXEC-BACKUP-PLEXUS::COM:::135.1.104.196,root,plexus9000,/FTPRoot;

===========UPGRADE SP-A/B (SP-A active)==========================

ED-EQPT::SP-B:::SWVERSION=6.2.1.2.SP.4
CPY-MEM::SP-B;
RMV-EQPT::SP-B;
wait 5 minutes
EXEC-RUN-UPGRADE:ihlcsgw6:SP-B
RST-EQPT::SP-B;

ED-EQPT::SP-A:::SWVERSION=6.2.1.2.SP.4
EXEC-LS:::::-l,/Telica/swCPU/CurrRel;
CPY-MEM::SP-A;
SW-TOPROTN-EQPT::SP-A:::norst;
EXEC-RUN-UPGRADE:ihlcsgw6:SP-A
RST-EQPT::SP-A;

===============UPDATING STANDBY IOMs============

