ED-EQPT::IOM-9:::SWVERSION=6.2.1.2.P3;
ED-EQPT::IOM-10:::SWVERSION=6.2.1.2;
ED-EQPT::IOM-17:::SWVERSION=6.2.1.2.P3;

CPY-MEM::IOM-7;
CPY-MEM::IOM-9;
CPY-MEM::IOM-10;
CPY-MEM::IOM-17;

RST-EQPT::IOM-7;
RST-EQPT::IOM-9;
RST-EQPT::IOM-10;
RST-EQPT::IOM-17;

