EXEC-LS:::::-1;
EXEC-TAR-EXTRACT::COM:::6.2.1.2.SP.2.9_cpu.tar.gz;
EXEC-TAR-EXTRACTSTBY::COM:::/home1/6.2.1.2.SP.2.9_cpu.tar.gz,/home1/;


